{
  "name": "planoras",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "pnpm codegen && next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:seed": "tsx src/core/database/seeds/index.ts",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:b": "graphql-codegen --config config/codegen.backend.ts",
    "codegen:f": "graphql-codegen --config config/codegen.frontend.ts",
    "prebuild": "pnpm prisma:generate && pnpm codegen:b",
    "clix:b": "bash scripts/clix_backend/cli.sh",
    "prepare": "husky",
    "clix:f": "bash scripts/clix_frontend/cli.sh"
  },
  "dependencies": {
    "@apollo/client": "^4.0.5",
    "@apollo/client-integration-nextjs": "^0.13.1",
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/schema": "^10.0.25",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "6.16.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dataloader": "^2.2.3",
    "es-toolkit": "^1.39.10",
    "graphql": "^16.11.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/client-preset": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.2",
    "husky": "^9.1.7",
    "prisma": "^6.16.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
